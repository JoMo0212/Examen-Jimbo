<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Add</servlet-name>
        <servlet-class>Add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consulta</servlet-name>
        <servlet-class>Consulta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Borrar</servlet-name>
        <servlet-class>Borrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar</servlet-name>
        <servlet-class>Editar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar2</servlet-name>
        <servlet-class>Editar2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>verificarUsuarios</servlet-name>
        <servlet-class>verificarUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consultarmun</servlet-name>
        <servlet-class>.Consultarmun</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add</servlet-name>
        <url-pattern>/Add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consulta</servlet-name>
        <url-pattern>/Consulta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Borrar</servlet-name>
        <url-pattern>/Borrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar</servlet-name>
        <url-pattern>/Editar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar2</servlet-name>
        <url-pattern>/Editar2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>verificarUsuarios</servlet-name>
        <url-pattern>/verificarUsuarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consultarmun</servlet-name>
        <url-pattern>/Consultarmun</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
